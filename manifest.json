{
  "manifest_version": 2,
  "name": "HeaderToken",
  "version": "1.0",
  "icons": {
    "16": "images/empty_16.png",
    "32": "images/empty_32.png",
    "48": "images/empty_48.png",
    "64": "images/empty_64.png",
    "128": "images/empty_128.png"
  },
  "content_scripts": [
    {
      "matches": [ "https://localhost","http://localhost"],
      "js": [ "content_scripts/end.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "images/empty_16.png",
      "32": "images/empty_32.png",
      "48": "images/empty_48.png",
      "64": "images/empty_64.png",
      "128": "images/empty_128.png"
    },
    "default_title": "HeaderToken",
    "default_popup": "popup/popup.html"
  }
}